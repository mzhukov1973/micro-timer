    "startOriginalCRASW": "react-scripts start",
    "buildOriginalCRASW": "react-scripts build",
    "startCreateCustomSWFile": "react-scripts start && cra-append-sw --skip-compile --mode dev     ./src/serviceWorker-custom.js",
